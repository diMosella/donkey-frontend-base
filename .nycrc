{
  "include": [
    "src/**/*.js",
    "src/**/*.jsx"
  ],
  "exclude": [
    "src/**/*.spec.js",
    "test/testSetup.js"
  ],
  "extension": [
    ".js",
    ".jsx"
  ],
  "reporter": [
    "lcov",
    "text"
  ],
  "require": "babel-core/register",
  "sourceMap": false,
  "instrument": false,
  "cache": true,
  "all": true,
  "check-coverage": true,
  "report-dir": "./coverage"
}
